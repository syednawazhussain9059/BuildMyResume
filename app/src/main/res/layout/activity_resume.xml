<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ScrollView1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ResumeActivity"
    tools:layout_editor_absoluteX="1dp"
    tools:layout_editor_absoluteY="38dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/ConstraintLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="38dp">

        <TextView
            android:id="@+id/Education1"
            android:layout_width="115dp"
            android:layout_height="56dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="20dp"
            android:text="@string/EducationQ"
            android:textAlignment="viewEnd"
            android:textColor="#E8A616"
            android:textStyle="bold"
            app:layout_constraintEnd_toStartOf="@+id/divider2"
            app:layout_constraintTop_toBottomOf="@+id/description29"
            tools:ignore="TextContrastCheck" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="45dp"
            android:layout_height="53dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="12dp"
            android:contentDescription="TODO"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView"
            app:srcCompat="@drawable/mail"
            tools:ignore="ImageContrastCheck" />

        <TextView
            android:id="@+id/job_role9"
            android:layout_width="215dp"
            android:layout_height="30dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:text="TextView"
            android:textSize="20sp"
            app:autoSizeTextType="none"
            app:layout_constraintStart_toEndOf="@+id/divider2"
            app:layout_constraintTop_toBottomOf="@+id/description9" />

        <TextView
            android:id="@+id/name9"
            android:layout_width="379dp"
            android:layout_height="59dp"
            android:layout_marginTop="20dp"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:text="@string/textview4"
            android:textColor="#E8A616"
            android:textSize="32sp"
            app:autoSizeMaxTextSize="32sp"
            app:autoSizeMinTextSize="12sp"
            app:autoSizeTextType="uniform"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.468"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/switch1"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/description9"
            android:layout_width="215dp"
            android:layout_height="93dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:paddingStart="5dp"
            android:paddingTop="5dp"
            android:paddingEnd="5dp"
            android:paddingBottom="5dp"
            android:text="TextView"
            android:textSize="20sp"
            app:autoSizeTextType="none"
            app:layout_constraintStart_toEndOf="@+id/divider2"
            app:layout_constraintTop_toBottomOf="@+id/divider" />

        <TextView
            android:id="@+id/company9"
            android:layout_width="215dp"
            android:layout_height="30dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="8dp"
            android:text="TextView"
            android:textSize="20sp"
            app:autoSizeTextType="none"
            app:layout_constraintStart_toEndOf="@+id/divider2"
            app:layout_constraintTop_toBottomOf="@+id/job_role9" />

        <TextView
            android:id="@+id/location9"
            android:layout_width="164dp"
            android:layout_height="53dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="20dp"
            android:autoLink="map"
            android:gravity="center|start"
            android:paddingStart="5dp"
            android:paddingTop="5dp"
            android:paddingEnd="5dp"
            android:paddingBottom="5dp"
            android:text="@string/textview6"
            app:autoSizeMaxTextSize="15sp"
            app:autoSizeMinTextSize="12sp"
            app:autoSizeTextType="uniform"
            app:layout_constraintStart_toEndOf="@+id/imageView2"
            app:layout_constraintTop_toBottomOf="@+id/name9" />

        <TextView
            android:id="@+id/phone9"
            android:layout_width="100dp"
            android:layout_height="53dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="20dp"
            android:autoLink="phone"
            android:gravity="center|left"
            android:maxLength="10"
            android:paddingStart="5dp"
            android:paddingTop="5dp"
            android:paddingEnd="5dp"
            android:paddingBottom="5dp"
            android:text="@string/textview5"
            app:layout_constraintStart_toEndOf="@+id/imageView"
            app:layout_constraintTop_toBottomOf="@+id/name9" />

        <TextView
            android:id="@+id/email9"
            android:layout_width="326dp"
            android:layout_height="53dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="12dp"
            android:autoLink="email"
            android:gravity="center|left"
            android:paddingStart="5dp"
            android:paddingTop="5dp"
            android:paddingEnd="5dp"
            android:paddingBottom="5dp"
            android:text="@string/textview1"
            app:autoSizeMaxTextSize="15sp"
            app:autoSizeMinTextSize="8sp"
            app:autoSizeTextType="uniform"
            app:layout_constraintStart_toEndOf="@+id/imageView4"
            app:layout_constraintTop_toBottomOf="@+id/location9" />

        <View
            android:id="@+id/divider"
            android:layout_width="390dp"
            android:layout_height="2dp"
            android:layout_marginTop="12dp"
            android:background="@color/white"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.571"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/email9" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="45dp"
            android:layout_height="53dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="20dp"
            android:contentDescription="@string/todo2"
            app:layout_constraintStart_toEndOf="@+id/phone9"
            app:layout_constraintTop_toBottomOf="@+id/name9"
            app:srcCompat="@drawable/location"
            tools:ignore="ImageContrastCheck" />

        <View
            android:id="@+id/divider2"
            android:layout_width="2dp"
            android:layout_height="475dp"
            android:layout_marginTop="70dp"
            android:layout_marginBottom="31dp"
            android:background="@color/black"
            app:layout_constraintBottom_toBottomOf="@+id/skills_data9"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.381"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/divider" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="9dp"
            android:layout_marginTop="18dp"
            android:layout_marginEnd="9dp"
            app:layout_constraintEnd_toStartOf="@+id/description9"
            app:layout_constraintStart_toEndOf="@+id/Profile"
            app:layout_constraintTop_toBottomOf="@+id/divider"
            app:srcCompat="@drawable/ic_baseline_circle_24" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="9dp"
            android:layout_marginTop="18dp"
            android:layout_marginEnd="9dp"
            app:layout_constraintEnd_toStartOf="@+id/job_role9"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/WorkExperience"
            app:layout_constraintTop_toBottomOf="@+id/description9"
            app:srcCompat="@drawable/ic_baseline_circle_24" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="9dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="9dp"
            app:layout_constraintEnd_toStartOf="@+id/degree9"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/Education1"
            app:layout_constraintTop_toBottomOf="@+id/description29"
            app:srcCompat="@drawable/ic_baseline_circle_24" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="9dp"
            android:layout_marginTop="21dp"
            android:layout_marginEnd="9dp"
            app:layout_constraintEnd_toStartOf="@+id/skills_data9"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/Skills"
            app:layout_constraintTop_toBottomOf="@+id/university9"
            app:srcCompat="@drawable/ic_baseline_circle_24" />

        <TextView
            android:id="@+id/Profile"
            android:layout_width="109dp"
            android:layout_height="26dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="20dp"
            android:text="@string/profile"
            android:textAlignment="viewEnd"
            android:textColor="#E8A616"
            android:textStyle="bold"
            app:layout_constraintEnd_toStartOf="@+id/divider2"
            app:layout_constraintTop_toBottomOf="@+id/divider"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/WorkExperience"
            android:layout_width="115dp"
            android:layout_height="56dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="20dp"
            android:text="@string/workExperience"
            android:textAlignment="viewEnd"
            android:textColor="#E8A616"
            android:textStyle="bold"
            app:layout_constraintEnd_toStartOf="@+id/divider2"
            app:layout_constraintTop_toBottomOf="@+id/description9"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/description29"
            android:layout_width="215dp"
            android:layout_height="80dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="8dp"
            android:text="TextView"
            android:textSize="20sp"
            app:autoSizeMaxTextSize="18sp"
            app:autoSizeMinTextSize="12sp"
            app:autoSizeTextType="none"
            app:layout_constraintStart_toEndOf="@+id/divider2"
            app:layout_constraintTop_toBottomOf="@+id/company9" />

        <TextView
            android:id="@+id/university9"
            android:layout_width="215dp"
            android:layout_height="30dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="8dp"
            android:text="TextView"
            android:textSize="20sp"
            app:autoSizeTextType="none"
            app:layout_constraintStart_toEndOf="@+id/divider2"
            app:layout_constraintTop_toBottomOf="@+id/degree9" />

        <TextView
            android:id="@+id/degree9"
            android:layout_width="215dp"
            android:layout_height="30dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:text="@string/textview3"
            android:textSize="20sp"
            app:autoSizeTextType="none"
            app:layout_constraintStart_toEndOf="@+id/divider2"
            app:layout_constraintTop_toBottomOf="@+id/description29" />

        <TextView
            android:id="@+id/skills_data9"
            android:layout_width="215dp"
            android:layout_height="100dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="20dp"
            android:paddingStart="5dp"
            android:paddingTop="5dp"
            android:paddingEnd="5dp"
            android:paddingBottom="5dp"
            android:text="TextView"
            android:textSize="20sp"
            app:autoSizeMaxTextSize="20sp"
            app:autoSizeMinTextSize="12sp"
            app:autoSizeTextType="none"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/divider2"
            app:layout_constraintTop_toBottomOf="@+id/university9"
            app:layout_constraintVertical_bias="0.0" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="45dp"
            android:layout_height="53dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="20dp"
            android:contentDescription="TODO"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/name9"
            app:srcCompat="@drawable/phone"
            tools:ignore="ImageContrastCheck" />

        <TextView
            android:id="@+id/Skills"
            android:layout_width="115dp"
            android:layout_height="56dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="20dp"
            android:text="@string/Skills"
            android:textAlignment="viewEnd"
            android:textColor="#E8A616"
            android:textStyle="bold"
            app:layout_constraintEnd_toStartOf="@+id/divider2"
            app:layout_constraintTop_toBottomOf="@+id/university9"
            tools:ignore="TextContrastCheck" />

        <Switch
            android:id="@+id/switch1"
            android:layout_width="83dp"
            android:layout_height="59dp"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="16dp"
            android:text="Dark Mode"
            android:textAllCaps="false"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="UseSwitchCompatOrMaterialXml" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</ScrollView>